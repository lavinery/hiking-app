{
  "name": "hiking-gear-recommendation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "typecheck": "tsc --noEmit --incremental false",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "db:setup": "prisma generate && prisma migrate dev && prisma db seed",
    "db:reset": "prisma migrate reset --force && prisma db seed",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start:production": "NODE_ENV=production next start",
    "clean": "rm -rf .next out dist coverage",
    "validate": "npm run typecheck && npm run lint:check",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.4.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "5.10.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.358.0",
    "next": "15.5.0",
    "next-auth": "^4.24.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.50.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "15.5.0",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prisma": "5.10.2",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}